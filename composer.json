{
    "name": "chrgriffin/laravel-defer",
    "description": "Automatically defer loading your images until after page load in Laravel.",
    "keywords": ["laravel", "framework", "defer images", "defer image loading", "image loading", "defer"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Griffin",
            "email": "christian.griffin@live.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/view": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "autoload": {
        "psr-4": {
            "ChrGriffin\\LaravelDefer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrGriffin\\LaravelDefer\\LaravelDeferServiceProvider"
            ],
            "aliases": {
                "LaravelDefer": "ChrGriffin\\LaravelDefer\\Facades\\LaravelDefer"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
